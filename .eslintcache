egend\": {}\n      },\n      `chart-${props.id}`, am4charts.XYChart );\n\n    chart.current = x;\n\n    return () => {\n      x.dispose();\n    };\n  }, [] );\n\n  return <div id={`chart-${props.id}`} style={{ width: \"100%\", height: \"400px\"}}></div>\n}\n\nexport default Chart","/Users/elliotrichardson/dmprojects/react/react-mini-revenue/src/components/UI/Spinner/Spinner.js",["105"],"import React from 'react'\nimport styles from './Spinner.module.scss'\n\nfunction Spinner( props ) {\n  let showSpinner = props.loading ? 'block' : 'none'\n  return <div style={{display: 'block'}} className={styles.Spinner}>\n          <div className={styles.Circle}>\n            <div className={styles.Circle2}></div>\n          </div>\n        </div>\n}\n\nexport default Spinner","/Users/elliotrichardson/dmprojects/react/react-mini-revenue/src/components/Charts/ChartPie.js",["106"],"import React, {\n  useRef,\n  useLayoutEffect\n} from 'react'\nimport * as am4core from \"@amcharts/amcharts4/core\";\nimport * as am4charts from \"@amcharts/amcharts4/charts\";\nimport am4themes_animated from \"@amcharts/amcharts4/themes/animated\";\n\nam4core.useTheme( am4themes_animated );\n\nfunction ChartPie( props ) {\n  let dataSet = props.data\n  let valueProp = props.value\n  let categoryProp = props.category\n  const chart = useRef( null );\n\n  useLayoutEffect( () => {\n      let x = am4core.createFromConfig( {\n          \"series\": [ {\n            \"type\": \"PieSeries\",\n            \"dataFields\": {\n              \"value\": valueProp,\n              \"category\": categoryProp\n            }\n          } ],\n          \"data\": dataSet,\n        },\n        `chart-${props.id}`,\n        am4charts.PieChart );\n\n      chart.current = x;\n      console.log( 'pie:', chart );\n      return () => {\n        x.dispose();\n      };\n    },\n        [] );\n\n  return <div id={`chart-${props.id}`} style={{ width: \"100%\", height: \"400px\"}}></div>\n}\n\nexport default ChartPie","/Users/elliotri