     \"valueY\": \"Revenue\",\n              \"categoryX\": \"Split\"\n            },\n            \"name\": \"Revenue\"\n        },\n          {\n            \"type\": \"LineSeries\",\n            \"dataFields\": {\n              \"valueY\": \"Events\",\n              \"categoryX\": \"Split\"\n            },\n            \"name\": \"Events\"\n          }\n        ],\n\n        // Add data\n        \"data\": dataSet,\n        // And, for a good measure, let's add a legend\n        \"legend\": {},\n        \"cursor\": {}\n      },\n      `chart-${props.id}`, am4charts.XYChart );\n\n    chart.current = x;\n\n    return () => {\n      x.dispose();\n    };\n  }, [] );\n\n  return <div id={`chart-${props.id}`} style={{ width: \"100%\", height: \"400px\"}}></div>\n}\n\nexport default Chart","/Users/elliotrichardson/dmprojects/react/react-mini-revenue/src/components/UI/Spinner/Spinner.js",["113"],"import React from 'react'\nimport styles from './Spinner.module.scss'\n\nfunction Spinner( props ) {\n  let showSpinner = props.loading ? 'block' : 'none'\n  return <div style={{display: 'block'}} className={styles.Spinner}>\n          <div className={styles.Circle}>\n            <div className={styles.Circle2}></div>\n          </div>\n        </div>\n}\n\nexport default Spinner","/Users/elliotrichardson/dmprojects/react/react-mini-revenue/src/components/Charts/ChartPie.js",["114"],"import React, {\n  useRef,\n  useLayoutEffect\n} from 'react'\nimport * as am4core from \"@amcharts/amcharts4/core\";\nimport * as am4charts from \"@amcharts/amcharts4/charts\";\nimport spiritedaway from \"@amcharts/amcharts4/themes/spiritedaway\";\nimport am4themes_animated from \"@amcharts/amcharts4/themes/animated\";\n\nam4core.useTheme( spiritedaway );\nam4core.useTheme( am4themes_animated );\n\nfunction ChartPie( props ) {\n  let dataSet = props.data\n  let valueProp = props.value\n  let categoryProp = props.category\n  const chart = useRef( null );\n\n  useLayoutEffect( () => {\n      let x = am4core.createFromConfig( {\n          \"series\": [ {\n            \"type\": \"PieSeries\",\n            \"dataFields\": {\n              \"value\": valueProp,\n              \"category\": categoryProp\n            },\n            \"slices\": {\n              \"tooltipText\": \"[bold]{category}:[/] {value.percent.formatNumber('#.0')}%\",\n              \"cornerRadius\": 5\n            },\n            \"labels\": {\n              \"fontSize\": 11,\n              \"text\": \"{value.percent.formatNumber('#.0')}%\"\n            },\n            \"ticks\": {\n              \"disabled\": true\n            },\n            \"alignLabels\": false\n          } ],\n          \"innerRadius\": \"50%\",\n          \"data\": dataSet,\n        },\n        `chart-${props.id}`,\n        am4charts.PieChart );\n\n      chart.current = x;\n      console.log( 'pie:', chart );\n      return () => {\n        x.dispose();\n      };\n    },\n        [] );\n\n  return <div id={`chart-${props.id}`} style={{ width: \"100%\", height: \"400px\"}}></div>\n}\n\nexport default ChartPie","/Users/elliotrichardson/dmprojects/react/react-mini-revenue/src/components/UI/Paragraph/Paragraph.js",[],"/Users/elliotrichardson/dmprojects/react/react-mini-revenue/src/components/UI/PageTitle/PageTitle.js",[],"/Users/elliotrichardson/dmprojects/react/react-mini-revenue/src/assets/Animate.js",[],"/Users/elliotrichardson/dmprojects/react/react-mini-revenue/src/components/Toc/TocContent/TocContent.js",[],{"ruleId":"115","replacedBy":"116"},{"ruleId":"117","replacedBy":"118"},{"ruleId":"119","severity":1,"message":"120","line":5,"column":3,"nodeType":"121","messageId":"122","endLine":5,"endColumn":8},{"ruleId":"119","severity":1,"message":"123","line":5,"column":8,"nodeType":"121","messageId":"122","endLine":5,"endColumn":13},{"ruleId":"119","severity":1,"message":"124","line":8,"column":3,"nodeType":"121","messageId":"122","endLine":8,"endColumn":12},{"ruleId":"119","severity":1,"message":"125","line":39,"column":13,"